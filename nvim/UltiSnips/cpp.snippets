snippet print "print"
std::cout << $1 << std::endl;
endsnippet

snippet main "main"
int main(int argc, char** argv) {
	return 0;
}
endsnippet

snippet fun "function def" b
${2:void} ${1:name}($3){
	${4:${VISUAL}}
}
endsnippet

snippet for "for"
for($1; $2; $3)
{
	$4
}
endsnippet

snippet class "class" b
class ${1:Class}{
	public:
		$1();
		$1(const $1&);
		$1& operator=(const $1&);
		~$1();

	private:
		$3
};
endsnippet
